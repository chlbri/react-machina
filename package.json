{
  "name": "@core_chlbri/zstate",
  "version": "0.0.5",
  "description": "A combinaison between zustand and xstate...",
  "author": "chlbri <bri_lvi@icloud.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "pnpm run test && pnpm build:-test",
    "build:-test": "pnpm run tsc && pnpm run lint",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx,yml,json}\"",
    "lint": "pnpm prettier && pnpm eslint -- --fix",
    "jest:clear": "jest --clearCache",
    "rinit": "ts-node \"./wconfig/rinit.ts\" & pnpm pnpm:install",
    "npm:prepublish": "ts-node \"./wconfig/pack.ts\" && pnpm install",
    "npm:publish": "git checkout main && pnpm npm:prepublish && pnpm publish --access public --no-git-checks && git clean -fd && git checkout -- . && pnpm pnpm:install && ts-node \"./wconfig/clean.ts\"",
    "pnpm:install": "pnpm install",
    "publish:all": "pnpm run rinit && pnpm git-publish && pnpm npm:publish",
    "upgrade:core": "pnpm upgrade @core_chlbri/core",
    "upgrade": "pnpm upgrade",
    "git-push": "git push --follow-tags",
    "git-publish": "pnpm test && publish"
  },
  "devDependencies": {
    "@core_chlbri/core": "^0.1.1-dc",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/edit-json-file": "^1.6.1",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/object.omit": "^3.0.0",
    "@types/omit": "^1.0.0",
    "@types/react": "^17.0.37",
    "@types/react-test-renderer": "^17.0.1",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "dayjs": "^1.10.7",
    "edit-json-file": "^1.6.2",
    "eslint": "^8.4.1",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "immer": "^9.0.7",
    "jest": "^27.4.3",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^1.2.0",
    "nanoid": "^3.1.30",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.1.0",
    "typescript": "^4.5.2",
    "xstate": "^4.26.1",
    "zustand": "^3.6.7"
  },
  "dependencies": {
    "object.omit": "^3.0.0"
  },
  "peerDependencies": {
    "xstate": "^4.26.1",
    "zustand": "^3.6.7"
  },
  "repository": "https://github.com/chlbri/react-machina.git"
}
